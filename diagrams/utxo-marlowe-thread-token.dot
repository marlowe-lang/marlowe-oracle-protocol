                <TABLE CELLSPACING="0" CELLPADDING="4" BORDER="0" CELLBORDER="0">
                  <TR>
                    <TD align="left"><b>currency</b></TD>
                    <TD align="left" balign="left">Marlowe</TD>
                  </TR>
                  <TR>
